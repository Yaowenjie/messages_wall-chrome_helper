{
  "manifest_version": 2,

  "name": "WeChat Messages Wall Beta",
  "description": "WeChat Messages Wall",
  "version": "0.0.1",

  "browser_action" : {
    "default_popup" : "popup.html"
  },

  "permissions" : [
    "storage"
  ],

  "content_scripts" : [
    {
      "matches": ["https://mp.weixin.qq.com/*"],
      "css": ["contentscript.css"],
      "js": ["jquery-1.11.1.min.js", "contentscript.js", "underscore-min.js"]
    }
  ]
}
